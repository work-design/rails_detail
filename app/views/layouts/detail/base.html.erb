<%= render layout: 'layouts/application_head' do %>
  <body class="as-body">
    <%= render 'nav_bar' %>
    <%= render 'shared/alert' %>
    <main class="columns is-centered as-main container">
      <div class="column is-one-quarter">
        <%= render 'nav' %>
      </div>
      <div class="column">
        <%= yield %>
      </div>
    </main>
    <%= render 'footer' %>
    <div id="modal"></div>
  </body>
<% end %>
